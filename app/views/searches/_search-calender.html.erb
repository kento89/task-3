<%= form_with url: search_carender_search_path, method: :get, remote: true do |f| %>
    <%= f.date_field :date %>
    <%= f.hidden_field :user_id, :value => @user.id %>
    <%= f.submit "検索", class: 'btn btn-primary' %>
    
<% end  %>

